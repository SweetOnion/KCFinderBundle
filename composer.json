{
    "name": "ikadoc/kcfinder-bundle",
    "type": "symfony-bundle",
    "description": "Provides a KCFinder integration for CKEditor for your Symfony Project.",
    "keywords": [ "ckeditor", "kcfinder" ],
    "license": "MIT",
    "authors": [
        {
            "name": "Yannick Cenatiempo",
            "email": "ycenatiempo@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "symfony/dependency-injection": "^5.0",
        "symfony/framework-bundle": "^5.0"
    },
    "require-dev": {
        "symfony/yaml": "^5.0",
        "twig/twig": "^2.12|^3.0"
    },
    "suggest": {
        "twig/twig": "Allows to use Twig templates"
    },
    "autoload": {
        "psr-4": { "Ikadoc\\KCFinderBundle\\": "" }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
